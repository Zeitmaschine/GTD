func check_status():
	if(upgrade2.getUpgrade2Unlock() == false):
		set_disabled(true)
		get_node(\"Cost2\").text = \"Gesperrt\"
	if(upgrade2.getUpgrade2Unlock() == true && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade2 == false):
		get_node(\"Cost2\").text = \"500$\"
	if (get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money < 500 || get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade2 == true):
		set_disabled(true)
	if(get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money >= 500 && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade2 == false && upgrade2.getUpgrade2Unlock() == true):
		set_disabled(false)
"

