script/source = "extends Control

var money = 0
var upgrade1 = false
var upgrade2 = false

func _ready():
	set_as_toplevel(true)

func _process(delta):
	check_status()

func _on_TextureButton_pressed():
	visible = false

func _on_SpecialUpgradeButton_pressed():
	get_parent().get_parent().get_node(\".\").upgrade1 = true
	get_parent().get_parent().get_parent().get_node(\".\").money -= 350
	get_node(\"UpgradeBackround/SpecialUpgradeBackround/SpecialUpgradeButton/Cost\").text = \"Gekauft\"

func _on_SpecialUpgradeButton2_pressed():
	get_parent().get_parent().get_node(\".\").upgrade2 = true
	get_parent().get_parent().get_parent().get_node(\".\").money -= 500
	get_node(\"UpgradeBackround/SpecialUpgradeBackround2/SpecialUpgradeButton2/Cost2\").text = \"Gekauft\"


func _on_DamageButton_pressed():
	get_parent().get_parent().get_node(\".\").attackpowerlvl +=  1
	get_parent().get_parent().get_parent().get_node(\".\").money -= 250
	if(get_parent().get_parent().get_node(\".\").attackpowerlvl ==  5):
		get_node(\"UpgradeBackround/StatUpgradeText/DamageText\").text = \"MAX\"

func _on_FirerateButton_pressed():
	get_parent().get_parent().get_node(\".\").fireratelvl +=  1
	get_parent().get_parent().get_parent().get_node(\".\").money -= 250
	if(get_parent().get_parent().get_node(\".\").fireratelvl ==  5):
		get_node(\"UpgradeBackround/StatUpgradeText/FirerateText\").text = \"MAX\"


func _on_RangeButton_pressed():
	get_parent().get_parent().get_node(\".\").attackrangelvl +=  1
	get_parent().get_parent().get_node(\".\").set_range()
	get_parent().get_parent().get_parent().get_node(\".\").money -= 250
	if(get_parent().get_parent().get_node(\".\").attackrangelvl ==  5):
		get_node(\"UpgradeBackround/StatUpgradeText/RangeText\").text = \"MAX\"

func check_status():
	if(get_parent().get_parent().get_node(\".\").attackpowerlvl ==  5 || get_parent().get_parent().get_parent().get_node(\".\").money < 250):
		get_node(\"UpgradeBackround/StatUpgradeContainer/DamageButton\").set_disabled(true)
	if(get_parent().get_parent().get_parent().get_node(\".\").money >= 250 && get_parent().get_parent().get_node(\".\").attackpowerlvl <  5):
		get_node(\"UpgradeBackround/StatUpgradeContainer/DamageButton\").set_disabled(false)
	if(get_parent().get_parent().get_node(\".\").fireratelvl ==  5 || get_parent().get_parent().get_parent().get_node(\".\").money < 250):
		get_node(\"UpgradeBackround/StatUpgradeContainer/FirerateButton\").set_disabled(true)
	if(get_parent().get_parent().get_parent().get_node(\".\").money >= 250 && get_parent().get_parent().get_node(\".\").fireratelvl <  5):
		get_node(\"UpgradeBackround/StatUpgradeContainer/FirerateButton\").set_disabled(false)
	if(get_parent().get_parent().get_node(\".\").attackrangelvl ==  5 || get_parent().get_parent().get_parent().get_node(\".\").money < 250):
		get_node(\"UpgradeBackround/StatUpgradeContainer/RangeButton\").set_disabled(true)
	if(get_parent().get_parent().get_parent().get_node(\".\").money >= 250 && get_parent().get_parent().get_node(\".\").attackrangelvl <  5):
		get_node(\"UpgradeBackround/StatUpgradeContainer/RangeButton\").set_disabled(false)


"

[sub_resource type="GDScript" id=2]
script/source = "extends VBoxContainer

export var currentDamage = 1
export var currentFirerate = 1
export var currentRange = 1


func _process(delta):
	currentDamage = get_parent().get_parent().get_parent().get_parent().attackpowerlvl
	currentFirerate = get_parent().get_parent().get_parent().get_parent().fireratelvl
	currentRange = get_parent().get_parent().get_parent().get_parent().attackrangelvl
	$CurrentDamage.text = str(currentDamage)
	$CurrentFirerate.text = str(currentFirerate)
	$CurrentRange.text = str(currentRange)
"

[sub_resource type="GDScript" id=3]
script/source = "extends Button

var money = 0
var upgrade1

func _process(delta):
	money = get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money
	upgrade1 = get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\")
	check_status()

func check_status():
	if(upgrade1.getUpgrade1Unlock() == false):
		set_disabled(true)
		get_node(\"Cost\").text = \"Gesperrt\"
	if(upgrade1.getUpgrade1Unlock() == true && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == false):
		get_node(\"Cost\").text = \"350$\"
	if (get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money < 350 || get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == true):
		set_disabled(true)
	if(get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money >= 350 && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == false && upgrade1.getUpgrade1Unlock() == true):
		set_disabled(false)
"

