func _process(delta):
	get_node(\"RealDamage\").text = \"Schaden: \" +  str(get_parent().get_parent().get_parent().get_parent().attackpowerlvl )
	get_node(\"RealFirerate\").text = \"Feuerrate: \" +  str(stepify(1 / (get_parent().get_parent().get_parent().get_parent().firerate - get_parent().get_parent().get_parent().get_parent().fireratelvl  * 0.2), 0.01) ) + \" Schüsse/s\"
	get_node(\"RealRange\").text = \"Reichweite: \" +  str(((get_parent().get_parent().get_parent().get_parent().attackrangelvl * 0.1) + 0.9) * 100) + \" %\"
