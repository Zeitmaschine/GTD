script/source = "extends Button

var money = 0
var upgrade1
var price

#Variablen werden festgelegt
func _process(delta):
	price = get_parent().get_parent().get_parent().get_node(\".\").price4
	money = get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money
	upgrade1 = get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\")
	check_status()

#Es wird geprüft, wann man das Upgrade kaufen darf
func check_status():
	if(upgrade1.getUpgrade1Unlock() == false):
		set_disabled(true)
		get_node(\"Cost\").text = \"Gesperrt\"
	if(upgrade1.getUpgrade1Unlock() == true && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == false):
		get_node(\"Cost\").text = str(price) + \"$\"
	if (get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money < price || get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == true):
		set_disabled(true)
	if(get_parent().get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").money >= price && get_parent().get_parent().get_parent().get_parent().get_parent().get_node(\".\").upgrade1 == false && upgrade1.getUpgrade1Unlock() == true):
		set_disabled(false)
"

