script/source = "extends TextureButton


# Declare member variables here. Examples:
var running = false
var faster = false
var normalSpeedTexture = preload(\"res://Assets/Icons/Play_Icon_Normal.png\")
var normalSpeedTexturePressed = preload(\"res://Assets/Icons/Play_Icon_Normal_pressed.png\")
var fasterSpeedTexture = preload(\"res://Assets/Icons/Play_Icon_Faster.png\")
var fasterSpeedTexturePressed = preload(\"res://Assets/Icons/Play_Icon_Faster_pressed.png\")
var pausedSpeedTexture = preload(\"res://Assets/Icons/Play_Icon.png\")
var pausedSpeedTexturePressed = preload(\"res://Assets/Icons/Play_Icon_pressed.png\")


# Called when the node enters the scene tree for the first time.
func _ready():
	pass # Replace with function body.


func _process(delta):
	check_status()

#Knopfdruck-Methoden
func _on_PlayButton_pressed():
	if get_tree().get_current_scene().get_node(\"GameHandler\").waverunning == false:
		get_tree().get_current_scene().get_node(\"GameHandler\").waverunning = true
		
	if running == true:
		if faster == true:
			faster = false
		
		else:
			faster = true

#Überprüfung, ob man auf den Knopf drücken darf 
func check_status():
	if faster == true:
		
		texture_normal = fasterSpeedTexture
		texture_pressed = fasterSpeedTexturePressed
		Engine.time_scale = 2
		get_tree().get_current_scene().get_node(\"GameHandler\").fasterTime = true
		
	else:
		texture_normal = normalSpeedTexture
		texture_pressed = normalSpeedTexturePressed
		Engine.time_scale = 1
		get_tree().get_current_scene().get_node(\"GameHandler\").fasterTime = false
	
	if get_tree().get_current_scene().get_node(\"GameHandler\").waverunning == false && get_tree().get_current_scene().get_node(\"GameHandler\").victory == false:
		
		texture_normal = pausedSpeedTexture
		texture_pressed = pausedSpeedTexturePressed
		running = false
		faster = false
	if get_tree().get_current_scene().get_node(\"GameHandler\").waverunning == true:
		running = true
	
	

"

