script/source = "extends Control

var unigekauft

func _ready():
	#Preise und Variablen werden festgelegt
	get_node(\"ShopBackround/MathematikerButton/MathematikerText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").mathematikerPrice) +\"$\"
	get_node(\"ShopBackround/InformatikerButton/InformatikerText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").informatikerPrice) +\"$\"
	get_node(\"ShopBackround/MathebombenturmButton/BombenturmText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").bombenturmPrice) +\"$\"
	get_node(\"ShopBackround/WinkelninjaButton/WinkelninjaText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").winkelninjaPrice) +\"$\"
	get_node(\"ShopBackround/UniversitaetButton/UniversitaetText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").uniPrice) +\"$\"
	get_node(\"ShopBackround/PhysikerButton/PhysikerText\").text = str(get_tree().get_current_scene().get_node(\"GameHandler\").physikerPrice) +\"$\"
	unigekauft = get_tree().get_current_scene().get_node(\"GameHandler\").unigekauft
	
func _process(delta):
	check_status()

func _on_ExitButton_pressed():
	visible = false

	#Hotkey-Steuerung
func _input(event):
	if event.is_action_pressed(\"Shop\"):
		visible = not visible
	if(event.is_action_pressed(\"ui_cancel\")):
		visible = false


func _on_ShopButton_pressed():
	visible = true

	#Turmplatzierungen
func _on_MathematikerButton_pressed():
	visible = false
	get_tree().get_current_scene().get_node(\"GameHandler\").add_mathematiker()

func _on_MathematikerButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Mathematiker\"

func _on_InformatikerButton_pressed():
	visible = false
	get_tree().get_current_scene().get_node(\"GameHandler\").add_informatiker()

func _on_InformatikerButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Informatiker\"

func _on_MathebombenturmButton_pressed():
	visible = false
	get_tree().get_current_scene().get_node(\"GameHandler\").add_mathebombenturm()

func _on_MathebombenturmButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Mathebombenturm\"

func _on_WinkelninjaButton_pressed():
	visible = false
	get_tree().get_current_scene().get_node(\"GameHandler\").add_winkelninja()

func _on_WinkelninjaButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Winkelninja\"

func _on_UniversitaetButton_pressed():
	visible = false
	unigekauft = true
	get_tree().get_current_scene().get_node(\"GameHandler\").max_universitaet = true
	get_node(\"ShopBackround/UniversitaetButton/UniversitaetText\").text = \"Gekauft\"
	get_tree().get_current_scene().get_node(\"GameHandler\").add_universitaet()

func _on_UniversitaetButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Universität\"

func _on_PhysikerButton_pressed():
	visible = false
	get_tree().get_current_scene().get_node(\"GameHandler\").add_physiker()

func _on_PhysikerButton_mouse_entered():
	get_node(\"ShopBackround/ShopLabel2/TowerName\").text = \"Physiker\"

	#Überprüfung, ob die Türme überhaupt gekauft werden dürfen
func check_status():
	#Mathematiker
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money < get_tree().get_current_scene().get_node(\"GameHandler\").mathematikerPrice):
		get_node(\"ShopBackround/MathematikerButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money >= get_tree().get_current_scene().get_node(\"GameHandler\").mathematikerPrice):
		get_node(\"ShopBackround/MathematikerButton\").set_disabled(false)
	#Informatiker
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money < get_tree().get_current_scene().get_node(\"GameHandler\").informatikerPrice):
		get_node(\"ShopBackround/InformatikerButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money >= get_tree().get_current_scene().get_node(\"GameHandler\").informatikerPrice):
		get_node(\"ShopBackround/InformatikerButton\").set_disabled(false)
	#Mathebombenturm
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money < get_tree().get_current_scene().get_node(\"GameHandler\").bombenturmPrice):
		get_node(\"ShopBackround/MathebombenturmButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money >= get_tree().get_current_scene().get_node(\"GameHandler\").bombenturmPrice):
		get_node(\"ShopBackround/MathebombenturmButton\").set_disabled(false)
	#Winkelninja
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money < get_tree().get_current_scene().get_node(\"GameHandler\").winkelninjaPrice):
		get_node(\"ShopBackround/WinkelninjaButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money >= get_tree().get_current_scene().get_node(\"GameHandler\").winkelninjaPrice):
		get_node(\"ShopBackround/WinkelninjaButton\").set_disabled(false)
	#Universität
	if(unigekauft == true):
		get_node(\"ShopBackround/UniversitaetButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money < get_tree().get_current_scene().get_node(\"GameHandler\").uniPrice):
		get_node(\"ShopBackround/UniversitaetButton\").set_disabled(true)
	if(get_tree().get_current_scene().get_node(\"GameHandler\").money >= get_tree().get_current_scene().get_node(\"GameHandler\").uniPrice && unigekauft == false):
		get_node(\"ShopBackround/UniversitaetButton\").set_disabled(false)
	#Hotkey
	if(visible == true):
		get_tree().get_current_scene().get_node(\"GameHandler\").pauseAllowed = 0
	if(visible == false):
		get_tree().get_current_scene().get_node(\"GameHandler\").pauseAllowed = 1
"

