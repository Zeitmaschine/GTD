var health = 250
var gameover = false

func _ready():
	set_as_toplevel(true)

#Überprüfungen und OOP-Methoden, wann der Screen angezeigt wird
func getHealth():
	return health

func setHealth(healthy):
	health = healthy
	if (health <= 0):
		gameover = true
		get_tree().paused = not get_tree().paused
		visible = not visible

func getGameOver():
	return gameover

#Knopfdruck-Methoden
func _on_GameOverMen_pressed():
	gameover = false
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/MainMenu.tscn\")


func _on_GameOverNeutstart_pressed():
	gameover = false
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().reload_current_scene()
