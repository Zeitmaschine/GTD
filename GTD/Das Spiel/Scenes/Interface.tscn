script/source = "extends Control

var gameover = false

func _ready():
	set_as_toplevel(true)

#Variablen festlegen
func _process(delta):
	gameover = get_parent().get_parent().get_node(\"GameOverLayer/GameOver\").getGameOver()

#Hotkey-Steuerung
func _input(event):
	if event.is_action_pressed(\"cancel\") && (gameover == false) && get_tree().get_current_scene().get_node(\"GameHandler\").pauseAllowed == 1 && get_parent().get_parent().get_node(\"Winlayer/Win\").visible == false && get_parent().get_parent().get_node(\"Winlayer2/Win2\").visible == false:
		get_tree().paused = not get_tree().paused
		visible = not visible

#Knopfdruck-Methoden
func _on_PauseFortsetzen_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible

func _on_PauseNeustart_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_parent().get_parent().get_node(\"Winlayer/Win\").win = false
	get_parent().get_parent().get_node(\"Winlayer2/Win2\").win2 = false
	get_tree().reload_current_scene()

func _on_PauseBeenden_pressed():
	get_parent().get_parent().get_node(\"Winlayer/Win\").win = false
	get_parent().get_parent().get_node(\"Winlayer2/Win2\").win2 = false
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/MainMenu.tscn\")

func _on_PauseIcon_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible
"

