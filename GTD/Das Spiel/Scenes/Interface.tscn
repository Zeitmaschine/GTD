script/source = "extends Label

export var wave = 1

func _ready():
	text = str(wave)
	
func _process(delta):
	text = str(wave)
	if (wave < 0):
		text = str(0)

func getWave():
	return wave
	
func setWave(newWave):
	wave = newWave
"

[sub_resource type="GDScript" id=2]
script/source = "extends Control

var gameover = false

func _process(delta):
	gameover = get_parent().get_parent().get_node(\"GameOverLayer/GameOver\").getGameOver()

func _input(event):
	if event.is_action_pressed(\"ui_cancel\") and (gameover == false):
		get_tree().paused = not get_tree().paused
		visible = not visible

func _on_PauseFortsetzen_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible

func _on_PauseNeustart_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/SimpleLevel.tscn\")

func _on_PauseBeenden_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/MainMenu.tscn\")

func _on_PauseIcon_pressed():
	get_tree().paused = not get_tree().paused
	visible = not visible
"

[sub_resource type="GDScript" id=3]
script/source = "extends Control

var health = 250
var gameover = false

func getHealth():
	return health

func setHealth(healthy):
	health = healthy
	if (health <= 0):
		gameover = true
		get_tree().paused = not get_tree().paused
		visible = not visible

func getGameOver():
	return gameover

func _on_GameOverMen_pressed():
	gameover = false
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/MainMenu.tscn\")


func _on_GameOverNeutstart_pressed():
	gameover = false
	get_tree().paused = not get_tree().paused
	visible = not visible
	get_tree().change_scene(\"res://Scenes/SimpleLevel.tscn\")
"

